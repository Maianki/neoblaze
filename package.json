{
  "name": "neoblaze",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "watch": "npm-run-all --parallel watch:*",
    "watch:sass": "sass --no-source-map --watch src:dist/css/",
    "build:sass": "sass --no-source-map src:dist/css/",
    "build:prefix-css": "postcss dist/css/neoblaze.css -o dist/css/neoblaze.css --use autoprefixer --no-map",
    "build:minify-css": "cleancss -o dist/css/neoblaze.min.css dist/css/neoblaze.css",
    "build": "npm-run-all build:*",
    "start": "npm-run-all build:sass watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "clean-css-cli": "^5.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0",
    "sass": "^1.55.0"
  }
}
